                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#LodgingBusiness"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasStarRating"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Un hotel.</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
        <rdfs:sameAs>https://schema.org/Hotel</rdfs:sameAs>
    </owl:Class>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#LocalBusiness -->


    <owl:Class rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#LocalBusiness">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Place"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasAssessment"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0</xsd:minInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5</xsd:maxInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Un negocio físico como un restaurante o un hotel.</rdfs:comment>
        <rdfs:label>Negocio local</rdfs:label>
        <rdfs:sameAs>https://schema.org/LocalBusiness</rdfs:sameAs>
    </owl:Class>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#LodgingBusiness -->


