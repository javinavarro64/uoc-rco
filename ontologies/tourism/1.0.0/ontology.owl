    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#CulturalEvent -->


    <owl:Class rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#CulturalEvent">
        <rdfs:subClassOf rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Event"/>
        <rdfs:comment>Evento relacionado con la cultura.</rdfs:comment>
        <rdfs:label>Evento cultural</rdfs:label>
    </owl:Class>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Event -->


    <owl:Class rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Event">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Place"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasCelebrationDate"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Un evento con potencial turístico.</rdfs:comment>
        <rdfs:label>Evento</rdfs:label>
    </owl:Class>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#FoodEstablishment -->


    <owl:Class rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#FoodEstablishment">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#LocalBusiness"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#servesCuisine"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#CuisineType"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasAveragePrice"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Un negocio relacionado con la restauración.</rdfs:comment>
        <rdfs:label>Establecimiento de comida</rdfs:label>
        <rdfs:sameAs>https://schema.org/FoodEstablishment</rdfs:sameAs>
    </owl:Class>
    


