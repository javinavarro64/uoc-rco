    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasAssessment -->


    <owl:DatatypeProperty rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasAssessment">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Hotel"/>
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Restaurant"/>
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#TouristAttraction"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Establece un valor de valoración de un cliente.</rdfs:comment>
        <rdfs:label>tiene una valoración de</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasAveragePrice -->


    <owl:DatatypeProperty rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasAveragePrice">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Restaurant"/>
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#TransportationBusiness"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Establece un valor de precio medio de un establecimiento.</rdfs:comment>
        <rdfs:label>tiene un precio medio de</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasCelebrationDate -->


    <owl:DatatypeProperty rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasCelebrationDate">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment>Establece un valor de fecha de un evento.</rdfs:comment>
        <rdfs:label>se celebra el</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasLocationCoordinates -->


    <owl:DatatypeProperty rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasLocationCoordinates">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Establece un valor de coordenadas de localización de un lugar.</rdfs:comment>
        <rdfs:label>tiene coordenadas de localización</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasPriceNight -->


    <owl:DatatypeProperty rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasPriceNight">
        <rdfs:domain rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Establece un valor de precio por noche para un estblecimiento de alojamientos.</rdfs:comment>
        <rdfs:label>tiene un precio por noche de</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasStarRating -->


    <owl:DatatypeProperty rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasStarRating">
        <rdfs:domain rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#byte"/>
        <rdfs:comment>Establece una clasificación de estrellas para un hotel.</rdfs:comment>
        <rdfs:label>tiene una clasificación de estrellas de</rdfs:label>
    </owl:DatatypeProperty>
    


