        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#LocalBusiness"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasPriceNight"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Un negocio de alojamiento como un motel o un hotel.</rdfs:comment>
        <rdfs:label>Negocio de alojamiento</rdfs:label>
        <rdfs:sameAs>https://schema.org/LodgingBusiness</rdfs:sameAs>
    </owl:Class>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#LuxuryHotel -->


