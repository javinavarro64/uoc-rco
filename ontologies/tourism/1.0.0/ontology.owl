    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#City -->


    <owl:Class rdf:about="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#City">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://javinavarro64.github.io/uoc-rco/ontologies/tourism#hasLocationCoordinates"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>Una ciudad donde hacer turismo.</rdfs:comment>
        <rdfs:label>Ciudad</rdfs:label>
        <rdfs:sameAs>https://schema.org/City</rdfs:sameAs>
    </owl:Class>
    


    <!-- https://javinavarro64.github.io/uoc-rco/ontologies/tourism#CuisineType -->


