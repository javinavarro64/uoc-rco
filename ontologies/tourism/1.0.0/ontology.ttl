                  :hasPriceNight 200 ;
                  :hasStarRating "5"^^xsd:byte ;
                  rdfs:comment "Hotel del Mar ubicado en Cádiz." ;
                  rdfs:label "Hotel del Mar de Cádiz" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#HotelSolCadiz
:HotelSolCadiz rdf:type owl:NamedIndividual ,
                        :Hotel ;
               :isLocatedIn :Cadiz ;
               :hasAssessment 4.6 ;
               :hasPriceNight 99 ;
               :hasStarRating 4 ;
               rdfs:comment "Hotel Sol ubicado en Cádiz." ;
               rdfs:label "Hotel Sol Cádiz" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#ItalianCuisine
:ItalianCuisine rdf:type owl:NamedIndividual ,
                         :CuisineType ;
                rdfs:comment "Cocina típica de italia." ;
                rdfs:label "Cocina italiana" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#JapaneseCuisin
:JapaneseCuisin rdf:type owl:NamedIndividual ,
                         :CuisineType ;
                rdfs:comment "Cocina típica japonesa." ;
                rdfs:label "Cocina japonesa" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#MediterraneanCuisine
:MediterraneanCuisine rdf:type owl:NamedIndividual ,
                               :CuisineType ;
                      rdfs:comment "Cocina típica de la zona del mediterráneo." ;
                      rdfs:label "Cocina mediterránea" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#Metro
:Metro rdf:type owl:NamedIndividual ,
                :MeanOfTransportation ;
       rdfs:comment "El metro." ;
       rdfs:label "Metro" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#MexicanCuisine
:MexicanCuisine rdf:type owl:NamedIndividual ,
                         :CuisineType ;
                rdfs:comment "Cocina típica de mexico." ;
                rdfs:label "Cocina mexicana" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#MoviCadiz
:MoviCadiz rdf:type owl:NamedIndividual ,
                    :TransportationBusiness ;
           :offersMeanOfTransport :Metro ,
                                  :Tram ;
           :hasAveragePrice 2.93 ,
                            4.63 ;
           rdfs:comment "Empresa de transporte ubicada en Cádiz." ;
           rdfs:label "Movi Cádiz" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#PlayaCadiz
:PlayaCadiz rdf:type owl:NamedIndividual ,
                     :Beach ;
            :isLocatedIn :Cadiz ;
            :hasAssessment 4.6 ;
            rdfs:comment "Una playa de Cádiz." ;
            rdfs:label "Playa de Cádiz" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#PublicBicycle
:PublicBicycle rdf:type owl:NamedIndividual ,
                        :MeanOfTransportation ;
               rdfs:comment "La bicicleta." ;
               rdfs:label "Bicicleta" .


###  https://javinavarro64.github.io/uoc-rco/ontologies/tourism#RestauranteDeliciaCadiz
:RestauranteDeliciaCadiz rdf:type owl:NamedIndividual ,
                                  :Restaurant ;
                         :isLocatedIn :Cadiz ;
                         :servesCuisine :MediterraneanCuisine ,
